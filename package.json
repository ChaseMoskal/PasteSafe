{
  "name": "pastesafe",
  "version": "0.2.1",
  "description": "Web crypto app",
  "scripts": {
    "setup": "npm i -g typescript & npm i -g webpack",
    "compile": "tsc && webpack ./build/script.js build/script.bundle.js",
    "sass": "node-sass --source-map true source/style.scss build/style.css",
    "sass-watch": "node-sass --source-map true --watch source/style.scss build/style.css",
    "build": "npm run compile & npm run sass"
  },
  "author": "Chase Moskal",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^3.4.2"
  }
}
